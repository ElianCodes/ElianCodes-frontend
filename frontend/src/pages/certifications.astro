---
// Component Imports
import BaseHead from '../components/layout/BaseHead.astro';
import DefaultHeader from '../components/layout/DefaultHeader.vue';
import DefaultFooter from '../components/layout/DefaultFooter.tsx';
import Certifications from '../components/certifications/Certifications.astro';

const title = 'Elian Van Cutsem | Certifications';
const description = 'See my relevant Software Engineer Certifications here. Some of them are really easy to get and you should too!';
const permalink = 'https://www.elian.codes/certifications/';
---
<html lang="en">
  <head>
    <BaseHead title={title} description={description} permalink={permalink} />
  </head>
  <body>
    <DefaultHeader textMenu={true} client:load/>
    <Certifications />
    <DefaultFooter client:load/>
  </body>
</html>
